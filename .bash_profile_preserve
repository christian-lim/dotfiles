# Setting PATH for Python 3.5
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.5/bin:${PATH}"
export PATH

# Setting PATH for Python 2.7
# The orginal version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/2.7/bin:${PATH}"
export PATH

# added by Anaconda3 5.0.1 installer
export PATH="/Users/christianlim/anaconda3/bin:$PATH"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/christianlim/safespace/google-cloud-sdk/path.bash.inc' ]; then source '/Users/christianlim/safespace/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/christianlim/safespace/google-cloud-sdk/completion.bash.inc' ]; then source '/Users/christianlim/safespace/google-cloud-sdk/completion.bash.inc'; fi

if [ -f $(brew --prefix)/etc/bash_completion ]; then
        source $(brew --prefix)/etc/bash_completion
fi

export VIRTUALENVWRAPPER_PYTHON=/usr/local/bin/python3

export WORKON_HOME=$HOME/virtualenvs
source /usr/local/bin/virtualenvwrapper.sh
export PIP_VIRTUALENV_BASE=$WORKON_HOME

alias pip='pip3'
echo "Let's get this bread!"

export CLICOLOR=1
export LSCOLORS=ExFxCxDxBxegedabagacad
~                                        